
def add_month_yr(x)

import pandas as pd
import datetime

file = 'survey_data.csv'
x = pd.read_csv(file)
a = x['Timestamp']
s = []
for i in range(len(a.values)):
    new_list = datetime.datetime.strptime(a.values[i], "%m/%d/%Y %H:%M").strftime("%b-%Y")
    s.append(new_list)
x['month-yr'] = s
x = x[['ID','month-yr']]
x = x.set_index('ID')
return x